
{{#if overview_name}}
<h4 id="asset-name" class="margin-none">{{overview_name}}</h4>
{{else}}
	{{#if name}}
	<h4 id="asset-name" class="margin-none">{{name}}</h4>
	{{/if}}
{{/if}}
{{#if overview_version}}
<p id="asset-version" class="grey padding-none margin-none">{{t "Version"}} {{overview_version}}</p>
{{else}}
	{{#if version}}
	<p id="asset-version" class="grey padding-none margin-none">{{t "Version"}} {{version}}</p>
	{{/if}}
{{/if}}
{{#if overview_description}}
    <p id="asset-description" class="grey padding-none margin-none">{{t "Description"}} {{overview_description}}</p>
{{else}}
    {{#if description}}
        <p id="asset-description" class="grey padding-none margin-none">{{t "Description"}} {{description}}</p>
    {{/if}}
{{/if}}